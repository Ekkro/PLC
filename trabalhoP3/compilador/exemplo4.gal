enq(i != 0){
    se ((i % 2) == 1){
        cont = (cont + 1);
        escreveri(i);
        escrevers(" \n");
    }
    i = leri();
}

escrevers("Foram lidos ");
escreveri(cont);
escrevers(" impares \n");


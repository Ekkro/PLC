"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>N</keycap></keycombo> "
"</shortcut> <guimenu>Game</guimenu> <guimenuitem>New Game</guimenuitem>"
msgstr ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>N</keycap></keycombo> "
"</shortcut> <guimenu>Jeu</guimenu> <guimenuitem>Nouvelle partie</guimenuitem>"

#: index.docbook:191
#, no-c-format
msgid "Starts a new game."
msgstr "Commence une nouvelle partie."

#: index.docbook:191
#, no-c-format
msgid ""
"<shortcut> <keycap>P</keycap> </shortcut> <guimenu>Game</guimenu> "
"<guimenuitem>Pause/Continue</guimenuitem>"
msgstr ""
"<shortcut> <keycap>P</keycap> </shortcut> <guimenu>Jeu</guimenu> "
"<guimenuitem>Pause/Reprendre</guimenuitem>"

#: index.docbook:191
#, no-c-format
msgid "Pauses the game."
msgstr "Met la partie en pause."

#: index.docbook:191
#, no-c-format
msgid ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>Q</keycap></keycombo> "
"</shortcut> <guimenu>Game</guimenu> <guimenuitem>Exit</guimenuitem>"
msgstr ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>Q</keycap></keycombo> "
"</shortcut> <guimenu>Jeu</guimenu> <guimenuitem>Quitter</guimenuitem>"

#: index.docbook:191
#, no-c-format
msgid "Quits."
msgstr "Quitte."

